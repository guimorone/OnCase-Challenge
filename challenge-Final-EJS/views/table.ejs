<div class="col">
    <table class="table table-bordered" id="tabela">
        <thead>
          <tr>
            <th scope="col"></th>
            <th scope="col">First name</th>
            <th scope="col">Last name</th>
            <th scope="col" class="alignCenterText">Participation</th>
          </tr>
        </thead>
        <tbody>
            <% informations.forEach((element, index) => { %>
                <tr>
                    <th scope="row" class="alignCenterText"> <%= index + 1 %> </th>
                    <td> <%= element.fname %> </td>
                    <td> <%= element.lname %> </td>
                    <td class="alignCenterText"> <%= element.participation.toString() + '%' %> </td>
                </tr>
            <% }); %>
        </tbody>
      </table>
</div>